<?xml version="1.0" encoding="utf-8"?>
<resources>

  <style name="RangeSliderStyle" parent="Widget.MaterialComponents.Slider">
    <item name="materialThemeOverlay">@style/ThemeOverlay.Slider</item>
    <item name="thumbColor">@color/green</item>
    <item name="trackColorActive">@color/green</item>
    <item name="trackColorInactive">@color/lightgray</item>
  </style>

  <style name="ThemeOverlay.Slider" parent="">
    <item name="colorOnPrimary">@color/black</item>
    <item name="colorOnBackground">@android:color/white</item>
    <item name="colorSurface">@color/green</item>
  </style>

  <style name="tab_selected_text" parent="@android:style/TextAppearance.Widget.TabWidget">
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">14sp</item>
  </style>

  <style name="tab_unselected_text" parent="@android:style/TextAppearance.Widget.TabWidget">
    <item name="android:textSize">14sp</item>
  </style>


</resources>